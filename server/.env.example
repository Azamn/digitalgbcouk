# Application
NEXT_APP_URI="http://localhost:3000"
PORT=5030

# Database (Replace with actual credentials in `.env`)
DATABASE_URL="mysql://USER:PASSWORD@HOST:PORT/DATABASE"

# Authentication (Use real credentials in `.env`)
AUTH_EMAIL="your-email@example.com"
AUTH_PASS="your-email-password"

# Encryption Key (Use a secure key in `.env`)
ENCRYPTION_KEY="your-secure-encryption-key"

# Session Token (Replace with a real token in `.env`)
SESSION_TOKEN_KEY="your-session-token-key"

# Cloudinary (Replace with actual credentials in `.env`)
CLOUDINARY_CLOUD_NAME="your-cloud-name"
CLOUDINARY_API_KEY="your-cloud-api-key"
CLOUDINARY_API_SECRET="your-cloud-api-secret"

# AI API Key (Replace with a valid key in `.env`)
AI_PUBLISHABLE_KEY="your-ai-api-key"
